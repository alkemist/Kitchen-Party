{
  "name": "kitchen-party",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config -- --environment=dev && ng serve --host 0.0.0.0 --port 4200",
    "prod": "npm run config -- --environment=prod && ng serve --configuration production --host 0.0.0.0 --port 4201",
    "build": "npm run config -- --environment=prod && ng build --configuration production --base-href /",
    "watch": "ng build --watch --configuration development",
    "test": "jest --coverage",
    "ngcc-test": "npx ngcc && jest --coverage",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "config": "ts-node ./scripts/setenv.ts",
    "netlify": "netlify dev",
    "netlify:backup": "netlify functions:invoke backup --port 1234",
    "deploy": "git pull; yarn install; rm -rf wwww ; yarn build"
  },
  "private": true,
  "dependencies": {
    "@alkemist/compare-engine": "^1.3.6",
    "@alkemist/ngx-data-store": "^1.1.38",
    "@alkemist/ngx-form-supervisor": "^1.1.16",
    "@alkemist/ngx-state-manager": "^1.2.25",
    "@alkemist/smart-tools": "^1.1.17",
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.0",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.0",
    "@angular/forms": "^16.2.0",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "@google-cloud/firestore": "^5.0.2",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@ngxs/logger-plugin": "^3.8.2",
    "@ngxs/storage-plugin": "^3.8.2",
    "@ngxs/store": "^3.8.2",
    "dotenv": "^14.2.0",
    "firebase": "^9.6.3",
    "mailgun.js": "^5.0.2",
    "material-icons": "^1.10.6",
    "ng-mocks": "^13.0.3",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.1.2",
    "primeicons": "^6.0.1",
    "primeng": "^15.2.2",
    "rxjs": "~7.8.0",
    "stackdriver-errors-js": "^0.12.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular/cli": "16.2.0",
    "@angular/compiler-cli": "^16.2.2",
    "@angular/localize": "^16.2.2",
    "@ngxs/devtools-plugin": "^3.8.2",
    "@types/jest": "^27.4.0",
    "@types/node": "^20.12.7",
    "chai": "^4.3.6",
    "core-js": "2.5.x",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0",
    "jest-preset-angular": "^11.1.1",
    "netlify-cli": "^9.13.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "5.0"
  }
}
