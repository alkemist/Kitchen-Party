{
  "name": "kitchen-party",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run config -- --environment=dev && ng serve --host 0.0.0.0 --port 4020",
    "prod": "npm run config -- --environment=prod && ng serve --prod --host 0.0.0.0 --port 4021",
    "build": "npm run config -- --environment=prod && ng build",
    "watch": "ng build --watch --configuration development",
    "test-u": "jest",
    "test-e2e": "ng test",
    "reset": "rm -rf node_modules && rm yarn.lock && yarn install",
    "config": "ts-node ./scripts/setenv.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.0",
    "@angular/cdk": "^13.1.3",
    "@angular/common": "~13.1.0",
    "@angular/compiler": "^13.1.3",
    "@angular/core": "~13.1.0",
    "@angular/forms": "~13.1.0",
    "@angular/platform-browser": "~13.1.0",
    "@angular/platform-browser-dynamic": "~13.1.0",
    "@angular/router": "~13.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@ngxs/logger-plugin": "^3.7.3",
    "@ngxs/storage-plugin": "^3.7.3",
    "@ngxs/store": "^3.7.3",
    "dotenv": "^14.2.0",
    "firebase": "^9.6.3",
    "material-icons": "^1.10.6",
    "nosleep.js": "^0.12.0",
    "primeflex": "^3.1.2",
    "primeicons": "^5.0.0",
    "primeng": "^13.1.0",
    "rxjs": "~7.4.0",
    "stackdriver-errors-js": "^0.12.0",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.4",
    "@angular/cli": "~13.1.4",
    "@angular/compiler-cli": "~13.1.0",
    "@ngxs/devtools-plugin": "^3.7.3",
    "@types/jasmine": "~3.10.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "jasmine-core": "~3.10.0",
    "jest": "^27.5.1",
    "jest-preset-angular": "^11.1.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.5.2"
  }
}
